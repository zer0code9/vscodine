			],
			"settings": {
				"foreground": "#FF9A00"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#9A00FF"
			}
		},
		{
			"name": "Deleted",
			"scope": [
