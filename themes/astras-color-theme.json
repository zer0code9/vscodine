			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#FF0000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name",
