{
    "name": "vscodine",
    "displayName": "VSCodine",
    "description": "An extension or something",
    "version": "0.0.1",
    "engines": { "vscode": "^1.95.0" },
    "publisher": "SlashDEV",
    "license": "MIT",
    "repository": { "type": "git", "url": "https://github.com/zer0code9/vscodine.git" },
    "icon": "img/logo.png",
    "categories": [ "Other" ],
    "keywords": [ "snippet", "theme", "keymaps", "slashDEV", "VSCodine" ],
    "activationEvents": [ "onStartupFinished" ],
    "main": "./extension.js",
    "contributes": {
        "themes": [
            { "label": "Padora", "uiTheme": "vs-dark", "path": "./themes/padora-color-theme.json" },
            { "label": "Sonama", "uiTheme": "vs-dark", "path": "./themes/sonama-color-theme.json" }
        ],
        "viewsContainers": {
            "activitybar": [
                { "id": "vscodine", "title": "VSCodine", "icon": "img/icon.png" }
            ]
        },
        "views": {
            "vscodine": [
                { "id": "vscodine.sideBar", "name": "VSCodine", "contextualTitle": "VScodine" }
            ]
        },
        "viewsWelcome": [
            { "view": "vscodine.sideBar", "contents": "Something something something..." }
            
        ],
        "commands": [
            { "command": "vscodine.open", "title": "Open VSCodine" }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.95.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "eslint": "^8.47.0",
        "glob": "^10.3.3",
        "mocha": "^10.2.0",
        "typescript": "^5.1.6",
        "@vscode/test-electron": "^2.3.4"
  }
}
